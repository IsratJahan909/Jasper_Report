	<queryString language="SQL">
		<![CDATA[SELECT 
    o.orderNumber ,
    o.orderDate ,
    c.customerName ,
    p.productName ,
    od.quantityOrdered ,
    od.priceEach ,
    (od.quantityOrdered * od.priceEach) AS TotalPrice
FROM classicmodels.orders o
JOIN classicmodels.customers c ON o.customerNumber = c.customerNumber
